<!DOCTYPE html>
<html>
  <head>
    <title>Slacker</title>
    <%= csrf_meta_tags %>
    <%= csp_meta_tag %>
    <link rel="icon" href="<%= image_url('Slack_Mark.png') %>" />
    <link rel="stylesheet" href="https://use.typekit.net/gvq0uik.css"/>
    <%= stylesheet_link_tag    'application', media: 'all' %>
    <script type="text/javascript">
      window.triangleWarning = "<%= image_url('exclamation-triangle-solid.svg') %>";
      window.slackerRGBUrl ="<%= image_url('Slacker_RGB.png') %>";
    </script>
    <%= javascript_include_tag 'application' %>
  </head>

  <body>
    <%= yield %>
  </body>
</html>
